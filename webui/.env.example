# Configuração do servidor
NODE_ENV=production
PORT=3000
HOST=127.0.0.1

# Credenciais AMI (Asterisk Manager Interface)
AMI_HOST=127.0.0.1
AMI_PORT=5038
AMI_USER=admin
AMI_SECRET=your_ami_secret_here

# Credenciais ARI (Asterisk REST Interface)
ARI_HOST=127.0.0.1
ARI_PORT=8088
ARI_USER=admin
ARI_PASSWORD=your_ari_password_here

# JWT para autenticação
JWT_SECRET=your_random_jwt_secret_here
JWT_EXPIRES_IN=24h

# Usuário admin padrão
ADMIN_USER=admin
ADMIN_PASSWORD=changeme
